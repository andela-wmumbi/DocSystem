{
  "name": "docsystem",
  "version": "1.0.0",
  "description": "Document Management System",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src",
    "open:src": "nodemon  ./server --exec babel-node",
    "lint": "node_modules/.bin/esw webpack.config.* client server server.*",
    "lint:watch": "npm run lint -- --watch",
    "migrations": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "test:bend": "export NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate &&  babel-node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -R spec ./server/tests/*.spec.js",
    "test:fend": "export NODE_ENV=test && babel-node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --compilers js:babel-core/register client/tests/**/*.js",
    "e2e": "protractor ./tests/config.js",
    "e2e-driver": "webdriver-manager start",
    "coverage": "./node_modules/.bin/coveralls < coverage/lcov.info"
  },
  "mocha": {
    "rootDir": "./",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-wmumbi/DocSystem.git"
  },
  "author": "Winnie Mumbi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-wmumbi/DocSystem/issues"
  },
  "homepage": "https://github.com/andela-wmumbi/DocSystem#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "jest": "^20.0.4",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.3",
    "materialize-css": "^0.98.2",
    "mock-local-storage": "^1.0.2",
    "morgan": "^1.8.2",
    "nock": "^9.0.14",
    "npm-run-all": "^4.0.2",
    "object-hash": "^1.1.8",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
